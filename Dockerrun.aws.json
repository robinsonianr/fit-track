{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "robinsonir-react",
      "image": "robinsonir/robinsonir-react:latest",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "robinsonir-api",
      "image": "robinsonir/robinsonir-api:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        }
      ]
    }
  ]
}